/* pages/img/img.wxss */
.img {
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.img-area {
  width: 90%;
  height: 50%;
  border: 2rpx pink solid;
}
.bar {
  height: 200rpx;
  width: 100%;
  background: #000;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.bar-block {
  width: 100rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 0 20rpx;
}
.bar-square {
  width: 100rpx;
  height:100rpx;
  border: 4rpx #fff solid;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.line-row {
  width: 100%;
  height: 4rpx;
  background: #fff;
}
.line-column {
  width: 4rpx;
  height: 100%;
  background: #fff;
}
