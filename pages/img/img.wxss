/* pages/img/img.wxss */
.img {
  position: relative;
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: space-around;
  background: #000;
}
.img-area {
  position: absolute;
  top: 30px;
  left: 13%;
  width: 280px;
  height: 280px;
}
.bar {
  height: 200rpx;
  width: 100%;
  background: #000;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
  color: #fff;
}
.bar image{
  width: 100rpx;
  height: 100rpx;
}
.bar-block {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.line-assemble {
  position: relative;
  height: 280px;
  width: 280px;
}
.line-assemble>view {
  background: #000;
}
/* 分割线 cLine纵，rLine横 */
.cLine {
  width: 1px;
  height: 100%;
  position: absolute;
  z-index: 100;
}
.rLine {
  width: 100%;
  height: 1px;
  position: absolute;
  z-index: 100;
}
.fourLine view:first-child{
  top: 0;
  left: calc( 50% - 0.5px );
}
.fourLine view:last-child{
  top: calc( 50% - 0.5px);
  left: 0;
}
/* 九宫格 */
.nineLine .one{
  top: 0;
  left: calc( 33.3% - 0.5px );
}
.nineLine .two{
  top: 0;
  left: calc( 66.6% - 0.5px);
}
.nineLine .three{
  top: calc( 33.3% - 0.5px );
  left: 0;
}
.nineLine .four{
  top: calc( 66.6% - 0.5px );
  left: 0;
}
 /* 画布 */
.square-canvas {
  width: 280px;
  height: 280px;
  position: absolute;
  top: -100%;
  left: -100%;
  backgrund: red;
  z-index: 101;
}