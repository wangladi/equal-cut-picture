<!--pages/img/img.wxml-->
<view class='img'>
  <image src="{{url}}" class='img-area'></image> 
    <!-- <image src="{{url}}" mode="aspectFit"></image>  -->
  <view class='bar'>
    <view class='bar-block'>
      <view class='bar-square four-s'>
        <text class='line-row'></text>
        <text class='line-column'></text>
      </view>
      <text>2X2</text>
    </view>
    <view class='bar-block'>
        <view class='bar-square nine-s'>
          <text class='line'></text>
          <text class='line'></text>
          <text class='line'></text>
          <text class='line'></text>
        </view>
        <text>3X3</text>
      </view>
  </view>
  <button>保存图片</button>
</view>
